num=int(input("enter the value : "))
print("fibonacci series is : ")
def fibon(num):
           first=0
           second=1
           for i in range(0,num):
                      if i<=1:
                             result=i
                      else:            
                             result=first+second
                             first=second
                             second=result
                      print(result)
fibon(num) 


