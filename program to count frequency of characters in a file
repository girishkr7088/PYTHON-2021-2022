# program to count frequency of characters in a file
file=open("sample.txt","w")
file.write("hello world")
file.close() 
file=open("sample.txt","r")
data=file.read()
print(data)
file.close() 
print("h =",data.count("h")) 
print("e =",data.count("e")) 
print("l =",data.count("l")) 
print("o =",data.count("o")) 
print("w =",data.count("w")) 
print("r =",data.count("r")) 
print("d =",data.count("d"))  
